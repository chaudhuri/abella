(rule
  (targets abella_doc.js)
  (deps ../package.json ../package-lock.json
        ../tsconfig.json ../ts/abella_doc.ts)
  (action (progn
    (run npm run js))))

(rule
  (targets abella_doc.css)
  (deps ../package.json ../package-lock.json
        ../tailwind.config.js ../css/abella_doc.css)
  (action (run npm run css)))